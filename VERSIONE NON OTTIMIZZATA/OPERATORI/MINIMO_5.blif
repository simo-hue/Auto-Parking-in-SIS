# Restituisce il minimo tra due numeri da 5 bit.
.model MINIMO_5
.inputs A4 A3 A2 A1 A0  B4 B3 B2 B1 B0
.outputs O4 O3 O2 O1 O0

.subckt MINORE_5 A4=A4 A3=A3 A2=A2 A1=A1 A0=A0  B4=B4 B3=B3 B2=B2 B1=B1 B0=B0  O=A_MINORE_DI_B
.subckt MUX_S1_5 S=A_MINORE_DI_B  A4=B4 A3=B3 A2=B2 A1=B1 A0=B0  B4=A4 B3=A3 B2=A2 B1=A1 B0=A0  O4=O4 O3=O3 O2=O2 O1=O1 O0=O0

.search operatori/MINORE_5.blif
.search flusso/mux/MUX_S1_5.blif

.end
